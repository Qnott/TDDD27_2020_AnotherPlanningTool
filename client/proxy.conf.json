{
	"/auth/*": {
		"target": "http://express-server:8080",
		"secure": false,
		"changeOrigin": true,
		"logLevel": "debug",
		"pathRewrite": { "^/auth": "/auth" }
	},
	"/graphql": {
		"target": "http://express-server:8080",
		"secure": false,
		"changeOrigin": true,
		"logLevel": "debug",
		"pathRewrite": { "^/graphql": "/graphql" }
	}
}
